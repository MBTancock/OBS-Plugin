<UIPlugin bundlePath="static">
  <View type="cueSheet">
    <Property name="productId" value="obsPlugin"/>
    <Property name="view-env" value="production"/>
    <Property name="view-vendor" value="broadcastmediasolutions"/>
    <Property name="view-mode" value="factory"/>
    <Property name="@menuName" value="Cue Sheet Export"/>
    <Property name="showMenu" value="true"/>
    <Import type="js" path="js/cuesheet/cueSheet.js"/>
    <Import type="js" path="js/cuesheet/datamodel/cuesheetRequest.js"/>
    <Import type="js" path="js/cuesheet/datamodel/cuesheetResponse.js"/>
   </View>
  <View type="system-settings-obs-view">
    <Property name="settings-type" value="system-settings"/>
    <Property name="showMenu" value="false"/>
    <Property name="productId" value="obsPlugin"/>
    <Property name="view-env" value="administration"/>
    <Property name="@name" value="OBS"/>
    <Import type="js" path="js/configuration/SystemSettingsObsView.js"/>
    <Import type="js" path="js/configuration/datamodel/exportConfiguration.js"/>    <!--<Import type="css" path="css/styles.css"/>-->
  </View>
  <ViewExtension type="av-asset-list">
    <Import type="js" path="js/export/asset-menu.js"/>
    <Import type="js" path="js/export/datamodel/inewsRequest.js"/>
    <Import type="js" path="js/export/datamodel/inewsResponse.js"/>
  </ViewExtension>
</UIPlugin>
